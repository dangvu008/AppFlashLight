                android:dashWidth="5dp"/>
            <corners android:radius="5dp"/>
        </shape>
    </item>
    <item android:minLevel="6" android:maxLevel="7">
        <shape android:shape="rectangle" android:thickness="2dp" android:useLevel="true"  android:dashGap="5dp" android:dashWidth="5dp">
            <stroke android:width="10dp" android:color="#304FFE" android:dashGap="5dp"
                android:dashWidth="5dp"/>
            <corners android:radius="5dp"/>
        </shape>
    </item>
    <item android:minLevel="8" android:maxLevel="9">
